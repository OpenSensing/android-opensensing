<?xml version="1.0" encoding="utf-8"?>
<resources>
      <string name="local_pipeline">
        {"@type":"edu.mit.media.funf.pipeline.BasicPipeline",
         "name":"local_pipeline",
         "version":1,
          "archive": {
		    "@schedule": {"interval": 300},
            "compress": true
          },
          "upload": {
			"url": \"https://content.dropboxapi.com/1/files_put/auto/incoming/$FILENAME$?access_token=$ACCESS_TOKEN$\",
			"@schedule": {"interval": 600}
		 },
         "data":[
            "edu.mit.media.funf.probe.builtin.WifiProbe",
            "edu.mit.media.funf.probe.builtin.LocationProbe",
            {"@type": "edu.mit.media.funf.probe.builtin.AccelerometerSensorProbe", "@schedule": {"interval": 300, "duration": 10, "strict": false}},
            "edu.mit.media.funf.probe.builtin.BatteryProbe",
            {"@type": "edu.mit.media.funf.probe.builtin.AccelerometerFeaturesProbe", "@schedule": {"interval": 300, "duration": 10, "strict": false}},
            {"@type": "edu.mit.media.funf.probe.builtin.AccountsProbe", "@schedule": {"interval": 300, "duration": 10, "strict": false}},
            {"@type": "edu.mit.media.funf.probe.builtin.AndroidInfoProbe", "@schedule": {"interval": 300, "duration": 10, "strict": false}}
         ]
        }
    </string>
</resources>